import re

def check_password(pw):
    rules = {
        "length >= 12": len(pw) >= 12,
        "uppercase": re.search(r"[A-Z]", pw),
        "lowercase": re.search(r"[a-z]", pw),
        "digit": re.search(r"[0-9]", pw),
        "symbol": re.search(r"[^A-Za-z0-9]", pw),
    }

    for rule, ok in rules.items():
        print(f"{rule}: {'OK' if ok else 'FAIL'}")

if __name__ == "__main__":
    pwd = input("Enter password: ")
    check_password(pwd)
